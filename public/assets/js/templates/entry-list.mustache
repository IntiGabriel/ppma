{{#if models.length}}
    <table class="ui basic table">
        <thead>
        <tr>
            <th>Name</th>
            <th colspan="2">Tags</th>
        </tr>
        </thead>
        <tbody>
            {{#each models}}
                <tr>
                    <td>{{name}}</td>
                    <td><a href="#">internet</a>, <a href="#">bla</a></td>
                    <td>
                        <a can-click="edit"><i class="circular inverted small pencil icon"></i> </a>
                        <a can-click="delete"><i class="circular inverted small remove icon"></i> </a>
                    </td>
                </tr>
            {{/each}}
        </tbody>
    </table>
    <div id="entry-delete-modal" class="ui modal">
        <i class="close icon"></i>
        <div class="header">
            Delete Entry
        </div>
        <div class="content">
            Do you really want to delete this entry?
        </div>
        <div class="actions">
            <div class="ui red deny button"><i class="remove icon"></i> Nop!</div>
            <div class="ui green approve button"><i class="checkmark icon"></i> Yep!</div>
        </div>
    </div>
{{else}}
    <p>There are no entries. C'mon create some...</p>
{{/if}}
